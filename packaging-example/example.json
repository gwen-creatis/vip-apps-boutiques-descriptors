{
    "name": "Example",
    "description": "Your app description",
    "author": "Your name",
    "tool-version": "v0.0.1",
    "schema-version": "0.5",
    "command-line": "python3 /home/vip/example.py --input1=[INPUT1] --input2=[INPUT2] --output=[OUTNAME]",
    "container-image": {
        "image": "myname/example:v0.0.1",
        "index": "docker://",
        "type": "docker"
    },
    "inputs": [
        {
            "id": "input1",
            "name": "Input1",
            "type": "String",
            "value-key": "[INPUT1]"
        },
        {
            "id": "input2",
            "name": "Input2",
            "type": "File",
            "value-key": "[INPUT2]"
        },
        {
            "id": "outname",
            "name": "Outname",
            "type": "String",
            "value-key": "[OUTNAME]"
        }
    ],
    "output-files": [
        {
            "id": "outfile",
            "name": "Outfile",
            "optional": false,
            "path-template": "[OUTNAME]"
        }
    ]
}
